    "@tsed/cli-plugin-jest": "3.11.5",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
