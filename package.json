    "@tsed/core": "7.0.0-alpha.1",
    "@tsed/di": "7.0.0-alpha.1",
    "@tsed/exceptions": "7.0.0-alpha.1",
    "@tsed/json-mapper": "7.0.0-alpha.1",
    "@tsed/platform-express": "7.0.0-alpha.1",
    "@tsed/schema": "7.0.0-alpha.1",
    "@tsed/swagger": "7.0.0-alpha.1",
    "ajv": "^8.6.3",
    "axios": "^1.6.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
